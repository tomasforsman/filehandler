{
  "Serilog": {
    "Using":  [],
    "MinimumLevel": {
      "Default": "Verbose",
      "Override": {
        "Microsoft": "Verbose",
        "System": "Verbose",
        "FileHandler.System": "Verbose"
      }
    },
    "Enrich": ["FromLogContext", "WithMachineName", "WithProcessId", "WithThreadId"],
    "WriteTo": [
      {"Name":  "Console"},
      {
        "Name": "File",
        "Args": {
          "path": "W:\\Logs\\filehandlerlog.txt",
          "outputTemplate": "{Timestamp:G} {Message}{NewLine:1}{Exceptions:1}"
        }
      },
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://localhost:5341",
          "outputTemplate": "{Timestamp:G} {Message}{NewLine:1}{Exceptions:1}"
        }
      }
    ]
  },
  "AppConfig": {
    "ApplicationInsights": {
      "InstrumentationKey": ""
    },
    "MongoDb": {
      "ConnectionString": "mongodb://localhost",
      "DatabaseName": "filedb",
      "CollectionName": "states",
      "AttachmentsConnectionString": "mongodb://127.0.0.1",
      "AttachmentsDatabaseName": "attachments"
    },
    "RabbitMq": {
      "HostAddress": "localhost",
      "VirtualHost": "/",
      "Username": "",
      "Password": ""
    },
    "Queues": {
      "SubmitFileInfo": "submit-file-info",
      "Quartz": "quartz"
    }
  },
  "ConnectionStrings": {
    "microservices": ""
  }
}